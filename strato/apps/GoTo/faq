<script>
    $('.stoerer-title').bind('click', function(event) {
        event.stopPropagation();

        if ( $('.stoerer-content-container').height() >= 440 ) {
            $('.gradient_bottom_40').show();
        }

        $('.stoerer-content-container').fadeToggle();
    });

    $('body').bind('click', function() {
        $('.stoerer-content-container').fadeOut();
    });
</script>

