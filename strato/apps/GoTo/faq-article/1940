            $("#danke").toggle();
            });
            var ratingdaten = new Object();
            $("#hilfreich_nein").bind("click", function() {

                $("#bewertung").toggle();
                $("#nicht_hilfreich").toggle();
                $(".fb_radio").bind("click", function(){
                    $("#btn_feedback").removeClass("btn_grey");
                    $("#btn_feedback").removeClass("cursor_default");
                    $("#btn_feedback").addClass("btn_blue");
                    $("#btn_feedback").addClass("pointer");

                    $("#btn_feedback").bind("click", function(){
                        if(!$(this).hasClass("btn_grey")){

                            ratingdate             = [];
                            ratingdaten['article'] = 1940;
                            ratingdaten['button']  = $("input[name='information']:checked").attr("id");
                            ratingdaten['info']    = $("#feedback_ta").val();

                            $("#btn_feedback").addClass("btn_grey");
                            $("#btn_feedback").addClass("cursor_default");
