                            var myfn   = function bewertung_auswerten(code){
                                //ggf. RÃ¼ckgaben vorsehen oder Fehlerbehandlungen.
                            }
                            var senden = rating.setzen(ratingdaten, myfn);
                            $("#danke").toggle();
                            $("#nicht_hilfreich").toggle();
                        }
                   });
                });
            });
        });
