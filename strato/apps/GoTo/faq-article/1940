        $(document).ready(function () {
            $F.faq.js.helper.dateMagic( '.dateMagic', 'de' );

            eucookie.init('de');

            $('body').click(function(e) {
                if($(e.target).hasClass("bodyclick"))
                {
                    return false;
                }
                else{
                    $('#menuelooping').hide();
                    $("#categoryselection").removeClass("round_corner_5_top").addClass("round_corner_5");
                    $("#menue_opener_icon").removeClass("fa-chevron-circle-up").addClass("fa-chevron-circle-down");
                }
            });
            $('#categoryselection').bind("click", function() {
                $('#menuelooping').toggle();
                if(!$('#menuelooping').is(":hidden")){
                    $("#categoryselection").removeClass("round_corner_5").addClass("round_corner_5_top");
                    $("#menue_opener_icon").removeClass("fa-chevron-circle-down").addClass("fa-chevron-circle-up");
                }
                else{
                    $("#categoryselection").removeClass("round_corner_5_top").addClass("round_corner_5");
                    $("#menue_opener_icon").removeClass("fa-chevron-circle-up").addClass("fa-chevron-circle-down");
                };
            });

            $("#input_img").bind("click", function(){
                $("#suchform").submit();
            });

            $("[data-track='emos']").on('click', function () {
                var $link       = $(this),
                    campaign    = $link.attr('data-campaign');

                if (campaign) {
                    campaign       = emospro.siteid.replace(/\./g, "_") + '/' + campaign
                    emospro.marker = campaign;
                    window.emosPropertiesEvent( emospro );
                }
            });

            bind_logout();

            
        });

            function ksb_logout(element){
                var url = "/auth/ksb";
                var methode="DELETE";
                var result = myajax.run(url, methode);
                if(element.hasClass("js_reload_after_logout")){
                    location.href = "/";
                }
                $(".loginarea_second").removeClass("hidden");
                $(".logoutarea").addClass("hidden");
                $(".logoutarea").removeClass("js_reload_after_logout").find("span").removeClass("js_reload_after_logout");
            }
            function bind_logout(){
                $(".kunden_logout").bind("click", function(){
                    var element = $(this);
                    ksb_logout(element);
                });
            }


        
            window.emosTrackVersion = 2;

            //<![CDATA[
                var emospro = {};
                emospro.content = 'www.faq.strato.de/article/1940';
                emospro.pageId = '1940';
                emospro.siteid = 'www.faq.strato.de';
                emospro.langid = 'de';
                
                window.emosPropertiesEvent(emospro);
            //]]>
        




