                $('.video_layer, .utils_layer, .details').click(function (e) {
                    $F.ksb.utils.popup.init( $(this) );
                    e.preventDefault();
                });

                $(".contract").each(function () {
                    $(this).html(
                        storefront.ksb.utils.helper.moreTextShrink( $(this).html(), 20 )
                    )
                });

                $('.sf-sidebar-tab.feedback').live('click', function () {
                    storefront.ksb.utils.popup.init( $(this) );
                    return false;
                });

                $('.popover-tooltip').popover({
                    placement: 'right',
                    container: 'body',
                    html: true,
                    trigger: 'focus',
                    content: function () {
                        return $(this).next('.popover-content').html();
                    }
                });
            });
