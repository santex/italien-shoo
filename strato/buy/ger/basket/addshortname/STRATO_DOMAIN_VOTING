<script type="text/javascript" >

    open_basket_bulked();
    function open_basket_bulked(){

             
            $('#basket_bulked').modal({
        		closeHTML: "<a href='#' title='Close' class='modal-close'><img src='/buy_static/strato/img/ger/overlay/fancybox.png' alt='close'></a>",
        		overlayId: 'overlay_basket_bulked',
        		containerId: 'confirm-container_basket_bulked', 
        		onShow: function (dialog) {
        			var modal = this;
                    $("#confirm-container_basket_bulked").css({
                        "z-index" : "910000005",
                        "height" : "auto",
                        "width": "590px",
                        "left": "30%",
                        "top": "30%"
                    });
        
        			$('#yes', dialog.data[0]).click(function () {
        				// call the callback
        				if ($.isFunction(callback)) {
        					callback.apply();
        					
        				}
        				// close the dialog
        				modal.close(); // or $.modal.close();
        			});
        		}
        	});
                	      
    }
</script>

